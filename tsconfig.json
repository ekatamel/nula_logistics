{
    "compilerOptions": {
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["ES2019", "dom", "ESNext.String"],
        "module": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true
    }
}
